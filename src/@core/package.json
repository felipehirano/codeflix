    "@types/uuid": "^8.3.4",
    "create-ts-index": "^1.14.0",
    "jest": "^29.0.3",
    "lodash": "^4.17.21",
    "ts-node": "^10.9.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.3",
